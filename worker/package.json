{
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/parser": "^7.20.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/uuid": "^9.0.7",
    "@types/redis": "^4.0.11",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "babel-plugin-tsconfig-paths-module-resolver": "^1.0.3",
    "babel-preset-minify": "^0.5.2",
    "eslint": "^8.0.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.2.0",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "sca": "yarn audit",
    "lint": "eslint src tests",
    "lint:fix": "eslint --fix src tests",
    "test:integration:quiet": "jest --silent integration",
    "test:integration:verbose": "jest --silent --verbose integration",
    "test:unit:quiet": "jest --silent --coverage unit",
    "test:unit:verbose": "jest --silent --verbose --coverage unit",
    "start:prisma": "prisma generate && prisma migrate deploy",
    "start:dev": "yarn start:prisma && ts-node-dev -r tsconfig-paths/register src/main.ts",
    "start:prod": "yarn start:prisma && node dist/main.js",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored"
  },
  "dependencies": {
    "@prisma/client": "^5.8.0",
    "csv-parser": "^3.0.0",
    "express": "^4.20.0",
    "moment": "^2.30.1",
    "prisma": "^5.8.0",
    "redis": "^4.6.12",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "resolutions": {
    "json5": ">=1.0.2",
    "micromatch": ">=4.0.8",
    "braces": ">=3.0.3",
    "body-parser": ">=1.20.3",
    "send": ">=0.19.0 ",
    "path-to-regexp": ">=0.1.10"
  }
}
